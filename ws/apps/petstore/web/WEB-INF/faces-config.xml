<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/j2ee/web-facesconfig_1_2.xsd"
    version="1.2">

  <!-- ========== Application Wide Features ================================ -->

  <!-- ========== Managed Beans ============================================ -->
  
  
    <managed-bean>
      <managed-bean-name>CreditCardBean</managed-bean-name>
      <managed-bean-class>com.sun.javaee.blueprints.petstore.CreditCardBean</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      
       <managed-property>
       <property-name>number</property-name>
       <property-class>java.lang.String</property-class>
       <value>111-2222-3333-111</value>
      </managed-property>
      
      
      <managed-property>
       <property-name>name</property-name>
       <property-class>java.lang.String</property-class>
       <value>Duke</value>
      </managed-property>
      <managed-property>
       <property-name>street</property-name>
       <property-class>java.lang.String</property-class>
       <value>121 Main Street</value>
      </managed-property>
      <managed-property>
       <property-name>city</property-name>
       <property-class>java.lang.String</property-class>
       <value>Santa Clara</value>
      </managed-property>
      <managed-property>
       <property-name>state</property-name>
       <property-class>java.lang.String</property-class>
       <value>CA</value>
      </managed-property>
      <managed-property>
       <property-name>zip</property-name>
       <property-class>java.lang.String</property-class>
       <value>95054</value>
      </managed-property>
  </managed-bean>
  
  <managed-bean>
      <managed-bean-name>AddressBean</managed-bean-name>
      <managed-bean-class>com.sun.javaee.blueprints.petstore.AddressBean</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
      <managed-property>
       <property-name>name</property-name>
       <property-class>java.lang.String</property-class>
       <value>Duke</value>
      </managed-property>
      <managed-property>
       <property-name>street</property-name>
       <property-class>java.lang.String</property-class>
       <value>121 Main Street</value>
      </managed-property>
      <managed-property>
       <property-name>city</property-name>
       <property-class>java.lang.String</property-class>
       <value>Santa Clara</value>
      </managed-property>
      <managed-property>
       <property-name>state</property-name>
       <property-class>java.lang.String</property-class>
       <value>CA</value>
      </managed-property>
      <managed-property>
       <property-name>zip</property-name>
       <property-class>java.lang.String</property-class>
       <value>95054</value>
      </managed-property>
  </managed-bean>
  
  <!-- ========== Navigation Rules ========================================= -->



  
</faces-config>

